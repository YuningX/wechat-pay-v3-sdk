{
  "name": "wechatpay-v3-sdk",
  "version": "2.0.0",
  "description": "wechatpay-v3-sdk",
  "files": [
    "dist",
    "types"
  ],
  "homepage": "https://github.com/wwog/wechatpay-v3-sdk",
  "bugs": "https://github.com/wwog/wechatpay-v3-sdk/issues",
  "types": "./types/index.d.ts",
  "main": "./dist/wechatpay-v3-sdk.umd.js",
  "module": "./dist/wechatpay-v3-sdk.es.js",
  "exports": {
    ".": {
      "import": "./dist/wechatpay-v3-sdk.es.js",
      "require": "./dist/wechatpay-v3-sdk.umd.js"
    }
  },
  "packageManager": "pnpm@7.29.3",
  "funding": "https://github.com/wwog",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "lint": "eslint --cache --ext .tsx,.ts,.js,.jsx .",
    "lint:fix": "eslint --cache --ext .tsx,.ts,.js,.jsx . --fix",
    "format": "prettier --ignore-path .eslintignore --write ./src/**/*.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "wechatpay",
    "v3",
    "sdk",
    "wxpay",
    "wx"
  ],
  "author": "wwog",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "axios": "^1.3.4",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "prettier": "^2.8.4",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "^0.29.2"
  }
}